@model List<List<int>>
<a><i class="fa fa-files-o fa-fw"></i> Quản Lý Công Việc Phòng<span class="fa arrow"></span></a>
<script src="~/Assets/vendor/jquery/jquery.min.js"></script>
<script src="~/Assets/vendor/bootstrap/js/bootstrap.min.js"></script>
<script src="~/Assets/vendor/metisMenu/metisMenu.min.js"></script>
<script src="~/Assets/vendor/raphael/raphael.min.js"></script>
<script src="~/Assets/vendor/morrisjs/morris.min.js"></script>
<script src="~/Assets/vendor/morrisjs/morris.js"></script>
<script src="~/Assets/dist/js/sb-admin-2.js"></script>
<script src="~/Assets/dist/js/taskShow.js"></script>
<style>


    /* Define what each icon button should look like */
    .button {
        color: white;
        display: inline-block; /* Inline elements with width and height. TL;DR they make the icon buttons stack from left-to-right instead of top-to-bottom */
        position: relative; /* All 'absolute'ly positioned elements are relative to this one */
        padding: 2px 5px; /* Add some padding so it looks nice */
    }

    /* Make the badge float in the top right corner of the button */
    .button__badge {
        background-color: #fa3e3e;
        border-radius: 2px;
        color: white;
        padding: 1px 3px;
        font-size: 10px;
        position: absolute; /* Position the badge within the relatively positioned button */
        top: 0;
        right: 0;
    }
</style>
<ul class="nav nav-second-level">
    <li>
        <a id="newPg">Công Việc Mới <span class="button">@Model[0][0] <span class="button__badge">@Model[0][1]</span></span> </a>
    </li>
    <li>
        @{
            <a id="inProPg" data-toggle="tooltip" data-placement="top" title="Co @Model[1][2] request">Công Việc Đang Làm <span class="button">@Model[1][0] <span class="button__badge">@(Model[1][1]+Model[1][2])</span></span></a>
        }

    </li>
    <li>
        <a id="requestPg">Công Việc Chờ Request <span class="button">@Model[2][0] <span class="button__badge">@Model[2][1]</span></span></a>
    </li>
    <li>
        <a id="commitPg">Công Việc Chờ Xác Nhận<span class="button">@Model[4][0] <span class="button__badge">@Model[4][1]</span></span></a>
    </li>
    <li>
        <a id="donePg">Công Việc Đã Hoàn Thành <span class="button">@Model[3][0] <span class="button__badge">@Model[3][1]</span></span></a>
    </li>
</ul>
@{

    var t = Model[Model.Count - 1][0].ToString();
}
<script>
                        $("#newPg").click(function () {
                            var rf = refresh();

                            $.ajax({
                                type: "POST",
                                url: "/Content/ShowContentPg?hosoId=@t",
                                success: function (data) {
                                    $('#tab').empty();
                                    $('#tab').append($(data));

                                }
                                });

                            });



                        $("#inProPg").click(function () {
                            var rf = refresh();
                            $.ajax({
                                type: "POST",

                                url: "/Content/ShowWorkingPg?hosoId=@t",
                                success: function (data) {
                                    $('#tab').empty();
                                    $('#tab').append($(data));
                                }
                            });

                        });

                        $("#requestPg").click(function () {
                            var rf = refresh();

                            $.ajax({
                                type: "POST",
                                url: "/Content/ShowRequest?hosoId=@t&ma=2",
                                success: function (data) {
                                    $('#tab').empty();
                                    $('#tab').append($(data));
                                }
                            });

                    });

                        $("#commitPg").click(function () {
                            $.ajax({
                                type: "POST",
                                url: "/Content/ShowCommitPg?hosoId=@t",
                                success: function (data) {
                                    $('#tab').empty();
                                    $('#tab').append($(data));
                                }
                            });
                            var rf = refresh();

                        });
                        $("#donePg").click(function () {
                            var rf = refresh();

                            $.ajax({
                                type: "POST",
                                url: "/Content/ShowDone?hosoId=@t&ma=2",
                                success: function (data) {
                                    $('#tab').empty();
                                    $('#tab').append($(data));
                                }
                            });

                        });

</script>